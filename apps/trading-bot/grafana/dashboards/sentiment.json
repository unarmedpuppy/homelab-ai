{
  "dashboard": {
    "title": "Trading Bot - Sentiment Analysis",
    "tags": ["trading-bot", "sentiment"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Provider Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(provider_requests_total{provider=~\"twitter|reddit|stocktwits|news|sec|google_trends|analyst_ratings|insider_trading\"}[5m])",
            "legendFormat": "{{provider}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 2,
        "title": "Provider Cache Hit Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "provider_cache_hit_rate{provider=~\"twitter|reddit|stocktwits|news|sec|google_trends|analyst_ratings|insider_trading\"}",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Hit Rate", "max": 1, "min": 0},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 3,
        "title": "Provider Response Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(provider_response_time_seconds_bucket{provider=~\"twitter|reddit|stocktwits|news|sec|google_trends|analyst_ratings|insider_trading\"}[5m]))",
            "legendFormat": "95th percentile - {{provider}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(provider_response_time_seconds_bucket{provider=~\"twitter|reddit|stocktwits|news|sec|google_trends|analyst_ratings|insider_trading\"}[5m]))",
            "legendFormat": "50th percentile - {{provider}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Seconds"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 4,
        "title": "Provider Errors",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(provider_errors_total{provider=~\"twitter|reddit|stocktwits|news|sec|google_trends|analyst_ratings|insider_trading\"}[5m])",
            "legendFormat": "{{provider}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Errors/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 5,
        "title": "Rate Limit Hits by Provider",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "rate(provider_rate_limit_hits_total{provider=~\"twitter|reddit|stocktwits|news|sec|google_trends|analyst_ratings|insider_trading\"}[5m])",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Hits/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 6,
        "title": "Provider Usage Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "sum by (provider) (provider_requests_total{provider=~\"twitter|reddit|stocktwits|news|sec|google_trends|analyst_ratings|insider_trading\"})",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "options": {
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "displayMode": "table",
            "placement": "right"
          }
        }
      },
      {
        "id": 7,
        "title": "Provider Success Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "sum(rate(provider_requests_total{provider=~\"twitter|reddit|stocktwits|news|sec|google_trends|analyst_ratings|insider_trading\", status=\"success\"}[5m])) / sum(rate(provider_requests_total{provider=~\"twitter|reddit|stocktwits|news|sec|google_trends|analyst_ratings|insider_trading\"}[5m])) * 100",
            "legendFormat": "Overall Success Rate",
            "refId": "A"
          },
          {
            "expr": "sum by (provider) (rate(provider_requests_total{provider=~\"twitter|reddit|stocktwits|news|sec|google_trends|analyst_ratings|insider_trading\", status=\"success\"}[5m])) / sum by (provider) (rate(provider_requests_total{provider=~\"twitter|reddit|stocktwits|news|sec|google_trends|analyst_ratings|insider_trading\"}[5m])) * 100",
            "legendFormat": "{{provider}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Success Rate %", "max": 100, "min": 0},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 8,
        "title": "Provider Metrics Summary",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "sum by (provider) (provider_requests_total{provider=~\"twitter|reddit|stocktwits|news|sec|google_trends|analyst_ratings|insider_trading\"})",
            "format": "table",
            "refId": "A"
          },
          {
            "expr": "provider_cache_hit_rate{provider=~\"twitter|reddit|stocktwits|news|sec|google_trends|analyst_ratings|insider_trading\"}",
            "format": "table",
            "refId": "B"
          },
          {
            "expr": "sum by (provider) (rate(provider_errors_total{provider=~\"twitter|reddit|stocktwits|news|sec|google_trends|analyst_ratings|insider_trading\"}[5m]))",
            "format": "table",
            "refId": "C"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {},
              "renameByName": {
                "Value #A": "Total Requests",
                "Value #B": "Cache Hit Rate",
                "Value #C": "Error Rate",
                "provider": "Provider"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Cache Hit Rate"},
              "properties": [
                {
                  "id": "unit",
                  "value": "percentunit"
                },
                {
                  "id": "decimals",
                  "value": 2
                },
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
                    "steps": [
                      {"color": "red", "value": null},
                      {"color": "yellow", "value": 0.5},
                      {"color": "green", "value": 0.7}
                    ]
                  }
                }
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Error Rate"},
              "properties": [
                {
                  "id": "unit",
                  "value": "short"
                },
                {
                  "id": "decimals",
                  "value": 3
                },
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
                    "steps": [
                      {"color": "green", "value": null},
                      {"color": "yellow", "value": 0.01},
                      {"color": "red", "value": 0.1}
                    ]
                  }
                }
              ]
            }
          ]
        }
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  }
}

