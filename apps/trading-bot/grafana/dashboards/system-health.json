{
  "dashboard": {
    "title": "Trading Bot - System Health",
    "tags": ["trading-bot", "system"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "system_cpu_usage_percent",
            "legendFormat": "CPU Usage",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Usage %", "max": 100, "min": 0},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"},
        "thresholds": [
          {"value": 0, "color": "green"},
          {"value": 70, "color": "yellow"},
          {"value": 90, "color": "red"}
        ]
      },
      {
        "id": 2,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "system_memory_usage_percent",
            "legendFormat": "Memory Usage %",
            "refId": "A"
          },
          {
            "expr": "system_memory_usage_bytes{type=\"used\"}",
            "legendFormat": "Used",
            "refId": "B"
          },
          {
            "expr": "system_memory_usage_bytes{type=\"available\"}",
            "legendFormat": "Available",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Usage %", "max": 100, "min": 0},
          {"format": "bytes", "label": "Bytes"}
        ],
        "xaxis": {"mode": "time"},
        "thresholds": [
          {"value": 0, "color": "green"},
          {"value": 70, "color": "yellow"},
          {"value": 85, "color": "red"}
        ]
      },
      {
        "id": 3,
        "title": "Disk Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "system_disk_usage_percent{mountpoint=\"/\"}",
            "legendFormat": "Disk Usage %",
            "refId": "A"
          },
          {
            "expr": "system_disk_usage_bytes{type=\"used\", mountpoint=\"/\"}",
            "legendFormat": "Used",
            "refId": "B"
          },
          {
            "expr": "system_disk_usage_bytes{type=\"free\", mountpoint=\"/\"}",
            "legendFormat": "Free",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Usage %", "max": 100, "min": 0},
          {"format": "bytes", "label": "Bytes"}
        ],
        "xaxis": {"mode": "time"},
        "thresholds": [
          {"value": 0, "color": "green"},
          {"value": 80, "color": "yellow"},
          {"value": 90, "color": "red"}
        ]
      },
      {
        "id": 4,
        "title": "System Uptime",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "system_uptime_seconds",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null}
              ]
            },
            "unit": "s"
          }
        }
      },
      {
        "id": 5,
        "title": "HTTP Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[5m]))",
            "legendFormat": "Total Requests/sec",
            "refId": "A"
          },
          {
            "expr": "sum(rate(http_requests_total{status_code=~\"2..\"}[5m]))",
            "legendFormat": "Success",
            "refId": "B"
          },
          {
            "expr": "sum(rate(http_requests_total{status_code=~\"4..\"}[5m]))",
            "legendFormat": "Client Errors",
            "refId": "C"
          },
          {
            "expr": "sum(rate(http_requests_total{status_code=~\"5..\"}[5m]))",
            "legendFormat": "Server Errors",
            "refId": "D"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 6,
        "title": "HTTP Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "rate(http_errors_total[5m])",
            "legendFormat": "{{error_type}} - {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Errors/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 7,
        "title": "Response Time Distribution",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "99th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Seconds"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 8,
        "title": "Provider Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "rate(provider_requests_total[5m])",
            "legendFormat": "{{provider}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 9,
        "title": "Provider Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "targets": [
          {
            "expr": "rate(provider_errors_total[5m])",
            "legendFormat": "{{provider}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Errors/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 10,
        "title": "Cache Hit Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40},
        "targets": [
          {
            "expr": "provider_cache_hit_rate",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Hit Rate", "max": 1, "min": 0},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 11,
        "title": "Rate Limit Hits",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40},
        "targets": [
          {
            "expr": "rate(provider_rate_limit_hits_total[5m])",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Hits/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 12,
        "title": "System Status Summary",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 48},
        "targets": [
          {
            "expr": "system_cpu_usage_percent",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "thresholds",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            },
            "unit": "percent",
            "decimals": 1
          }
        },
        "title": "CPU"
      },
      {
        "id": 13,
        "title": "Memory",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 48},
        "targets": [
          {
            "expr": "system_memory_usage_percent",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "thresholds",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 85}
              ]
            },
            "unit": "percent",
            "decimals": 1
          }
        }
      },
      {
        "id": 14,
        "title": "Disk",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 48},
        "targets": [
          {
            "expr": "system_disk_usage_percent{mountpoint=\"/\"}",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "thresholds",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 80},
                {"color": "red", "value": 90}
              ]
            },
            "unit": "percent",
            "decimals": 1
          }
        }
      },
      {
        "id": 15,
        "title": "Uptime",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 48},
        "targets": [
          {
            "expr": "system_uptime_seconds",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "none",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null}
              ]
            },
            "unit": "s"
          }
        }
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  }
}

