{
  "dashboard": {
    "title": "Trading Bot - Trading Dashboard",
    "tags": ["trading-bot", "trading"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Trades Executed",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(trades_executed_total[5m])",
            "legendFormat": "{{strategy}} - {{symbol}} - {{side}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Trades/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 2,
        "title": "Trades Rejected",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "rate(trades_rejected_total[5m])",
            "legendFormat": "{{reason}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Rejections/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 3,
        "title": "Trade Execution Duration",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(trade_execution_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile - {{strategy}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(trade_execution_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile - {{strategy}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Seconds"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 4,
        "title": "Order Fill Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(order_fill_time_seconds_bucket[5m]))",
            "legendFormat": "95th percentile - {{order_type}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(order_fill_time_seconds_bucket[5m]))",
            "legendFormat": "50th percentile - {{order_type}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Seconds"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 5,
        "title": "Slippage Distribution",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(slippage_percent_bucket[5m]))",
            "legendFormat": "95th percentile - {{side}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.05, rate(slippage_percent_bucket[5m]))",
            "legendFormat": "5th percentile - {{side}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.50, rate(slippage_percent_bucket[5m]))",
            "legendFormat": "Median - {{side}}",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Slippage %"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 6,
        "title": "Open Positions by Symbol",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "open_positions",
            "legendFormat": "{{symbol}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Quantity"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 7,
        "title": "Position P/L by Symbol",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "position_pnl",
            "legendFormat": "{{symbol}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "currencyUSD", "label": "Unrealized P/L"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 8,
        "title": "Portfolio Total P/L",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "portfolio_total_pnl",
            "legendFormat": "Total P/L",
            "refId": "A"
          },
          {
            "expr": "portfolio_daily_pnl",
            "legendFormat": "Daily P/L",
            "refId": "B"
          },
          {
            "expr": "portfolio_monthly_pnl",
            "legendFormat": "Monthly P/L",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "currencyUSD", "label": "P/L"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 9,
        "title": "Portfolio Total Value",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "targets": [
          {
            "expr": "portfolio_total_value",
            "legendFormat": "Portfolio Value",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "currencyUSD", "label": "Value"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 10,
        "title": "Broker Connection Status",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 40},
        "targets": [
          {
            "expr": "broker_connection_status",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [
              {
                "options": {
                  "0": {"text": "Disconnected", "color": "red"},
                  "1": {"text": "Connected", "color": "green"}
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "green", "value": 1}
              ]
            },
            "unit": "none"
          }
        }
      },
      {
        "id": 11,
        "title": "Total Trades Executed",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 40},
        "targets": [
          {
            "expr": "sum(increase(trades_executed_total[1h]))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null}
              ]
            },
            "unit": "short",
            "decimals": 0
          }
        }
      },
      {
        "id": 12,
        "title": "Rejection Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 40},
        "targets": [
          {
            "expr": "sum(rate(trades_rejected_total[5m])) / sum(rate(trades_executed_total[5m]) + rate(trades_rejected_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "thresholds",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            },
            "unit": "percent",
            "decimals": 2
          }
        }
      },
      {
        "id": 13,
        "title": "Total Open Positions",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 40},
        "targets": [
          {
            "expr": "sum(open_positions)",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "none",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "blue", "value": null}
              ]
            },
            "unit": "short",
            "decimals": 0
          }
        }
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  }
}

