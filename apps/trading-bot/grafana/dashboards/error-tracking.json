{
  "dashboard": {
    "title": "Trading Bot - Error Tracking",
    "tags": ["trading-bot", "errors"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Error Rate by Type",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(http_errors_total[5m])",
            "legendFormat": "{{error_type}} - {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Errors/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 2,
        "title": "HTTP Status Codes Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum by (status_code) (http_requests_total{status_code=~\"[45]..\"})",
            "legendFormat": "{{status_code}}",
            "refId": "A"
          }
        ],
        "options": {
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "displayMode": "table",
            "placement": "right"
          }
        }
      },
      {
        "id": 3,
        "title": "Provider Errors by Type",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(provider_errors_total[5m])",
            "legendFormat": "{{provider}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Errors/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 4,
        "title": "Provider Error Rate by Provider",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "sum by (provider) (rate(provider_errors_total[5m]))",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Errors/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 5,
        "title": "Error Rate vs Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(http_errors_total[5m]))",
            "legendFormat": "Error Rate",
            "refId": "A"
          },
          {
            "expr": "sum(rate(http_requests_total[5m]))",
            "legendFormat": "Request Rate",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Rate"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 6,
        "title": "Rate Limit Hits",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "rate(provider_rate_limit_hits_total[5m])",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Hits/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 7,
        "title": "Trades Rejected by Reason",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "rate(trades_rejected_total[5m])",
            "legendFormat": "{{reason}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Rejections/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 8,
        "title": "Error Summary Table",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "sum by (error_type, endpoint) (rate(http_errors_total[5m]))",
            "format": "table",
            "refId": "A"
          },
          {
            "expr": "sum by (provider, error_type) (rate(provider_errors_total[5m]))",
            "format": "table",
            "refId": "B"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {},
              "renameByName": {
                "Value #A": "HTTP Error Rate",
                "Value #B": "Provider Error Rate",
                "endpoint": "Endpoint",
                "provider": "Provider",
                "error_type": "Error Type"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.01},
                {"color": "red", "value": 0.1}
              ]
            },
            "unit": "short",
            "decimals": 3
          }
        }
      },
      {
        "id": 9,
        "title": "Total Errors (Last Hour)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 40},
        "targets": [
          {
            "expr": "sum(increase(http_errors_total[1h]))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 100}
              ]
            },
            "unit": "short",
            "decimals": 0
          }
        }
      },
      {
        "id": 10,
        "title": "Provider Errors (Last Hour)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 40},
        "targets": [
          {
            "expr": "sum(increase(provider_errors_total[1h]))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 100}
              ]
            },
            "unit": "short",
            "decimals": 0
          }
        }
      },
      {
        "id": 11,
        "title": "Rate Limit Hits (Last Hour)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 40},
        "targets": [
          {
            "expr": "sum(increase(provider_rate_limit_hits_total[1h]))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 20}
              ]
            },
            "unit": "short",
            "decimals": 0
          }
        }
      },
      {
        "id": 12,
        "title": "Trade Rejections (Last Hour)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 40},
        "targets": [
          {
            "expr": "sum(increase(trades_rejected_total[1h]))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 20}
              ]
            },
            "unit": "short",
            "decimals": 0
          }
        }
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  }
}

