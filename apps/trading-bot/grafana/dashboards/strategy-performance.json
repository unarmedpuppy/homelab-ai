{
  "dashboard": {
    "title": "Trading Bot - Strategy Performance",
    "tags": ["trading-bot", "strategy"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Signals Generated by Strategy",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum by (strategy) (rate(signals_generated_total[5m]))",
            "legendFormat": "{{strategy}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Signals/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 2,
        "title": "Signal Types Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum by (type) (signals_generated_total)",
            "legendFormat": "{{type}}",
            "refId": "A"
          }
        ],
        "options": {
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "displayMode": "table",
            "placement": "right"
          }
        }
      },
      {
        "id": 3,
        "title": "Strategy Evaluation Duration",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(strategy_evaluation_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile - {{strategy}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(strategy_evaluation_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile - {{strategy}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Seconds"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 4,
        "title": "Signal Confidence Distribution",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(signal_confidence_bucket[5m]))",
            "legendFormat": "95th percentile - {{strategy}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(signal_confidence_bucket[5m]))",
            "legendFormat": "50th percentile - {{strategy}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.05, rate(signal_confidence_bucket[5m]))",
            "legendFormat": "5th percentile - {{strategy}}",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Confidence", "max": 1, "min": 0},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 5,
        "title": "Strategy Win Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "strategy_win_rate",
            "legendFormat": "{{strategy}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Win Rate", "max": 1, "min": 0},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 6,
        "title": "Trades by Strategy",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "sum by (strategy) (rate(trades_executed_total[5m]))",
            "legendFormat": "{{strategy}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Trades/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 7,
        "title": "Signals by Symbol",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "sum by (symbol) (signals_generated_total)",
            "format": "table",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {},
              "renameByName": {
                "Value": "Total Signals",
                "symbol": "Symbol"
              }
            }
          }
        ]
      },
      {
        "id": 8,
        "title": "Strategy Performance Summary",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "strategy_win_rate",
            "format": "table",
            "refId": "A"
          },
          {
            "expr": "sum by (strategy) (signals_generated_total)",
            "format": "table",
            "refId": "B"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {},
              "renameByName": {
                "Value #A": "Win Rate",
                "Value #B": "Signals Generated",
                "strategy": "Strategy"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.5},
                {"color": "green", "value": 0.7}
              ]
            },
            "unit": "percentunit"
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Signals Generated"},
              "properties": [
                {
                  "id": "unit",
                  "value": "short"
                },
                {
                  "id": "decimals",
                  "value": 0
                }
              ]
            }
          ]
        }
      },
      {
        "id": 9,
        "title": "Evaluation Frequency by Strategy",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "sum by (strategy) (rate(strategy_evaluation_duration_seconds_count[5m]))",
            "legendFormat": "{{strategy}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Evaluations/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  }
}

